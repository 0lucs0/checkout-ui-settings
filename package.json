{
  "name": "checkout",
  "version": "0.0.1",
  "description": "Checkout project",
  "main": "index.ts",
  "repository": "",
  "author": "Luis C. Rojas <luis.rojas@itglobers.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "webpack --watch --mode=development --config ./webpack.config.js",
    "build": "webpack --mode=production --config ./webpack.config.js",
    "build:dev": "webpack --mode=development --config ./webpack.config.js",
    "dev:link": "yarn dev & vtex link",
    "lint": "eslint --cache --ext js,jsx,ts,tsx .",
    "format": "pretty-quick --write \"**/*.{ts,tsx,js,jsx,json,scss}\"",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "pretty-quick --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-env": "^7.21.5",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@types/jquery": "^3.5.16",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-loader": "^9.1.2",
    "conventional-changelog-angular": "^5.0.13",
    "conventional-changelog-cli": "^2.2.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jshint": "^2.13.6",
    "lint-staged": "^13.2.2",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2"
  },
  "dependencies": {}
}
