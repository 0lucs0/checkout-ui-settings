{
  "name": "checkout",
  "version": "0.0.1",
  "description": "Checkout project",
  "main": "index.js",
  "repository": "",
  "author": "Luis C. Rojas <luis.rojas@itglobers.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "webpack --watch --mode=development --config ./webpack.config.js",
    "build": "webpack --mode=production --config ./webpack.config.js",
    "build:dev": "webpack --mode=development --config ./webpack.config.js",
    "dev:link": "yarn dev & vtex link",
    "lint": "eslint --cache --ext js,jsx,ts,tsx .",
    "format": "pretty-quick --write \"**/*.{ts,tsx,js,jsx,json,scss}\"",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "pretty-quick --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/preset-env": "^7.21.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-vtex": "^15.0.2",
    "eslint-plugin-jquery": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "jshint": "^2.13.6",
    "lint-staged": "^13.2.1",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "sass": "^1.61.0",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.7",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {}
}
